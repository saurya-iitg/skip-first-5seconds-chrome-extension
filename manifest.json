{
    "manifest_version": 3,
    "name": "Video Skipper",
    "version": "1.0",
    "description": "Fast forward and skip the last few seconds of videos automatically.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["*://www.coursera.org/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["*://www.coursera.org/*"],
        "js": ["content.js"]
      }
    ]
  }
  