{
    "manifest_version": 3,
    "name": "Video Skipper",
    "version": "1.0",
    "description": "Fast forward and skip the last few seconds of videos automatically.",
    "permissions": [
      "activeTab",
      "webRequest",
      "webRequestBlocking"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "action": {
      "default_icon": "logo.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "options_page": "options.html"
  }
  